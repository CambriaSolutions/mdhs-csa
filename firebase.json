{
  "hosting": [
    {
      "target": "analytics",
      "public": "hosting/analytics/build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "testHarness",
      "public": "webchat-core/build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ],
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true,
      "host": "localhost",
      "port": 4000
    },
    "functions": {
      "host": "localhost",
      "port": 4600
    },
    "database": {
      "port": 9000
    },
    "pubsub": {
      "port": 8085
    },
    "hosting": {
      "port": 5000
    }
  },
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}